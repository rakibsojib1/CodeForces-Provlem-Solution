#include<iostream>
using namespace std;
int main(){
    int n,x;
    x = 0;
    cin>>n;
    string a;
    if(n>=1&&n<=150){
    for(int i=0; i<n; i++){
        cin>>a;
        if (a=="++X"){
             ++x;
        } else if(a == "X++"){
             x++;
        }else if (a=="--X"){
             --x;
        }else if (a=="X--"){
            x--;
        }
    }
    cout<<x<<endl;
    }
    return 0;
}
